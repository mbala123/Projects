@model IEnumerable<TaskLibrary.Models.History>

@{
    ViewData["Title"] = "GetByPriority";
}

<h1 class="mb-4 text-center" style="font-weight:bold">Get By Priority</h1>

@if (!Model.Any())
{
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        No History available.
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}
else
{
    <partial name="partialhistoryview" model="Model" />

}

<a asp-action="Index" class="btn btn-primary mt-3">Back to List</a>
